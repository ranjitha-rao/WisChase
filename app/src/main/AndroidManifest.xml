<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wischase">
    <uses-sdk android:minSdkVersion="14" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/quizwhiz_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />
        <activity
            android:name=".activity.TakeAQuiz"
            android:label="@string/title_activity_take_aquiz"
            android:theme="@style/AppTheme"></activity>
        <activity
            android:name=".activity.Addcategory"
            android:label="@string/title_activity_addcategory"
            android:theme="@style/AppTheme"></activity>
        <activity
            android:name=".activity.Optionscreen"
            android:label="@string/title_activity_optionscreen"
            android:theme="@style/AppTheme"></activity>
        <activity
            android:name=".activity.QuestionUpload"
            android:label="@string/title_activity_questionupload"
            android:theme="@style/AppTheme">
           </activity>
        <activity
            android:name=".activity.Selectionscreen"
            android:label="@string/title_activity_selectionscreen"
            android:theme="@style/AppTheme"></activity>
        <activity
            android:name=".activity.LogInScreen"
            android:label="@string/title_activity_take_aquiz"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.AnswerUpload"
            android:label="@string/title_activity_answer_upload"
            android:theme="@style/AppTheme">
        </activity>
      </application>
</manifest>
